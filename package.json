  "private": true,
  "homepage": "/bivariate-manager",
  "scripts": {
    "scripts": "jq .scripts package.json",
    "start": "cross-env-shell NODE_ENV=development webpack-dev-server --mode development --host 0.0.0.0 --disable-host-check --color",
    "build": "cross-env-shell NODE_ENV=production webpack",
    "preupgrade:k2": "node ./updatek2.js",
    "upgrade:k2": "npm update $(node ./updatek2.js)",
    "postinstall": "ln -s ../tsconfig.json ./node_modules/tsconfig.settings.json",
    "lint-staged": "lint-staged",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*/**/*.{ts, tsx}": [
      "eslint --quiet --fix"
    ],
    "*/**/*.{css}": [
      "stylelint"
    ],
    "*/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
